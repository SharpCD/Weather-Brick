<Loading>
    name: 'loading'
    MDFloatLayout:
        Image:
            id: img
            source: 'Data/Image//loading.png'

<LogIn>
    name: 'log_in'
    entry: entry
    email: email
    entry_btn:entry_btn
    password:password
    sign_up_btn:sign_up_btn
    MDFloatLayout:
        Image:
            id: img
            source: 'Data/Image//back.png'
        MDFloatLayout:
            size_hint: (.9, .3)
            pos_hint:{'center_x': .5, 'center_y': .5}
            canvas:
                Color:
                    rgb: rgba(196, 191, 196, 255)
                RoundedRectangle:
                    pos: self.pos
                    size: self.size
                    radius: [20, 20, 20, 20]
            TextInput:
                id:email
                multiline:False
                hint_text:'Email'
                font_size:
                background_color: 1, 1, 1, 0
                pos_hint:{'center_x': .55, 'center_y': .35}
                foreground_color: rgba(135, 135, 135, 255)
            MDFloatLayout:
                size_hint: (.9, .01)
                pos_hint:{'center_x': .5, 'center_y': .7}
                md_bg_color: rgba(178, 178, 178, 255)
            TextInput:
                id:password
                multiline:False
                hint_text:'Пароль'
                font_size:
                background_color: 1, 1, 1, 0
                pos_hint:{'center_x': .55, 'center_y': .19}
                foreground_color: rgba(135, 135, 135, 255)
                password: True
            MDFloatLayout:
                size_hint: (.9, .01)
                pos_hint:{'center_x': .5, 'center_y': .54}
                md_bg_color: rgba(178, 178, 178, 255)
            Button:
                id: entry_btn
                size_hint: (.9, .14)
                pos_hint:{'center_x': .5, 'center_y': .4}
                font_size:
                text: 'ВОЙТИ'
                background_color: 1, 1, 1, 0
                canvas.before:
                    Color:
                        rgb: rgba(158,153,163, 255)
                    RoundedRectangle:
                        pos: self.pos
                        size: self.size
                        radius: [18, 18, 18, 18]
            MDRoundFlatButton:
                id: sign_up_btn
                text: "ЗАРЕГИСТРИРОВАТЬСЯ"
                theme_text_color: "Custom"
                font_size:
                size_hint: (.9, .14)
                pos_hint:{ 'center_x': .5, 'center_y': .22}
                text_color: "white"
                line_color: "white"
            MDTextButton:
                text: "Пропустить"
                color: rgba(135, 135, 135, 255)
                pos_hint:{'center_x': .5, 'center_y': .07}
                on_press:
                    root.manager.current = 'menu_window'
                    root.manager.transition.direction = 'left'
        MDLabel:
            id: entry
            font_size:
            text: 'Вход'
            pos_hint:{'center_x': .93, 'center_y': .63}

